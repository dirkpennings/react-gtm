{
  "name": "react-gtm",
  "version": "2.0.0",
  "description": "React Google Tag Manager Module, forked from https://github.com/alinemorelli/react-gtm",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint .",
    "jest": "jest",
    "jest:watch": "jest watch",
    "test": "npm run lint --ignore build && npm run jest",
    "build": "babel src -d dist --ignore src/__tests__",
    "prepublish": "npm run build",
    "git:add:all": "git add .",
    "git:commit:autobuild": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.18.0",
    "enzyme": "3.3.0",
    "eslint": "3.9.0",
    "jest": "19.0.2",
    "pre-commit": "1.2.2",
    "react": "16.2.0",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "16.2.0",
    "sinon": "1.14.1"
  },
  "peerDependencies": {
    "react": "0.14.x || 16.2.0"
  },
  "pre-commit": [
    "build",
    "git:add:all"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vadorequest/react-gtm"
  },
  "author": {
    "name": "Vadorequest",
    "email": "vadorequest@example.com",
    "url": "https://github.com/vadorequest"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vadorequest/react-gtm/issues"
  },
  "homepage": "",
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "google tag manager",
    "tag manager",
    "gtm"
  ],
  "jest": {
    "testRegex": "__tests__.*\\.spec.js$",
    "collectCoverage": true
  },
  "dependencies": {
    "jest-cli": "25.1.0"
  }
}
